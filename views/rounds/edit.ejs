<!DOCTYPE html>
<html lang="en" dir="ltr">
    <head>
        <%- include ('../partials/head.ejs') %>
    </head>
    <body>
        <nav>
            <%- include ('../partials/nav.ejs') %>
        </nav>
        <blockquote>
            <h1>Edit Round</h1>
            <form class="" action="/scores/<%=score.id%>?_method=PUT" method="post">
                <label for="course">course</label>
                <input type="text" name="course" value="<%=score.course%>" /><br>
                <label for="score">score</label>
                <input type="text" name="score" value="<%=score.score%>"/><br>
                <label for="tees">tees</label>
                <input type="text" name="tees" value="<%=score.tees%>" /><br>
                <label for="date">date</label>
                <input type="text" name="date" value="<%=score.date%>" /><br>
                <br>
                <input type="submit" name="" value="Submit Log">
            </form>
        </blockquote>
        <script src="/js/app.js" charset="utf-8"></script>
    </body>
</html>
